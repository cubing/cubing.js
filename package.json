    },
    "./scramble": {
      "types": "./dist/lib/cubing/scramble/index.d.ts",
      "import": "./dist/lib/cubing/scramble/index.js"
    },
    "./search": {
      "types": "./dist/lib/cubing/search/index.d.ts",
